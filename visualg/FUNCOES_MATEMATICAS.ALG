Algoritmo "funcoes_matematicas"

Var
  x, y, z, a, b, c, d : real

Inicio
  x <- 2.0
  y <- 3.0
  z <- -7.0
  a <- Exp(x, y)   //Exp(base,expoente)
  b <- RaizQ(y)    //raiz quadrada
  c <- Pi          // vale Pi (3,1415...)
  d <- Abs(z)      // valor absoluto (sem o sinal)
  escreval(a)
  escreval(b)
  escreval(c)
  escreval(d)

Fimalgoritmo